{
  "name": "ku-sheet-backend",
  "version": "1.0.0",
  "description": "Backend API for KU SHEET - Study note sharing platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node prisma/seed.js",
    "postinstall": "npx prisma generate",
    "prisma:deploy": "npx prisma migrate deploy",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset",
    "db:push": "npx prisma db push",
    "db:drop-system-settings": "node scripts/drop_system_settings.js"
  },
  "keywords": [
    "study",
    "notes",
    "sharing",
    "university",
    "api",
    "prisma",
    "express",
    "nodejs"
  ],
  "author": "KU SHEET Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.0",
    "google-auth-library": "^10.1.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
  "multer": "^2.0.0",
  "nodemailer": "^7.0.5",
  "socket.io": "^4.7.5",
    "stripe": "^14.23.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.7",
    "prisma": "^6.14.0"
  }
}
